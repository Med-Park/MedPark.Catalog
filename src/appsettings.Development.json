{
  "App": {
    "name": "catalog-service"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "SeqOptions": {
    "Enabled": true,
    "ApiKey": null,
    "SeqServerUrl": "http://localhost:5340/"
  },
  "Logstash": {
    "Enabled": false,
    "LogstashgUrl": null
  },
  "Database": {
    "ConnectionString": "Server=LOCALHOST;Database=medpark-catalog-service;Trusted_Connection=True;MultipleActiveResultsets=true;ConnectRetryCount=5;ConnectRetryInterval=10;Connect Timeout=30;",
    "MockData": "False"
  },
  "mongo": {
    "connectionString": "mongodb://localhost:27017",
    "database": "medpark-catalog-service",
    "seed": true
  },
  "rabbitMq": {
    "namespace": "catalog-service",
    "retries": 3,
    "retryInterval": 2,
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost"
    ],
    "requestTimeout": "00:00:10",
    "publishConfirmTimeout": "00:00:01",
    "recoveryInterval": "00:00:10",
    "persistentDeliveryMode": true,
    "autoCloseConnection": true,
    "automaticRecovery": true,
    "topologyRecovery": true,
    "exchange": {
      "durable": true,
      "autoDelete": false,
      "type": "Topic"
    },
    "queue": {
      "autoDelete": false,
      "durable": true,
      "exclusive": false
    }
  },
  "redis": {
    "enabled": true,
    "connectionString": "localhost",
    "instance": "catalog:"
  }
}
